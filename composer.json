{
  "name": "jeeven/nepali-date-converter",
  "description": "Nepali (BS) ↔ English (AD) Date Converter for Laravel — by Jeeven Lamichhane.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jeeven\\NepaliDateConverter\\": "src/"
    }
  },
  "require": {
        "php": ">=8.0"
    },
  "extra": {
    "laravel": {
      "providers": [
        "Jeeven\\NepaliDateConverter\\NepaliDateServiceProvider"
      ],
      "aliases": {
        "NepaliDate": "Jeeven\\NepaliDateConverter\\Facades\\NepaliDate"
      }
    }
  }
}
